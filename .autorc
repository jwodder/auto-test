{
    "onlyPublishWithReleaseLabel": true,
    "baseBranch": "master",
    "author": "auto <auto@nil>",
    "noVersionPrefix": true,
    "plugins": [
        "git-tag",
        [
            "exec",
            {
                "version": "printf '$ARG_0=%s\\n' "$ARG_0" && bump2version --new-version $ARG_0 irrelevant"
            }
        ]
    ]
}
